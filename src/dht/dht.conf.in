[dht]
AUTOSTART = YES
@JAVAPORT@PORT = 2095
HOSTNAME = localhost
HOME = $SERVICEHOME
BINARY = gnunet-service-dht
ACCEPT_FROM = 127.0.0.1;
ACCEPT_FROM6 = ::1;
BUCKET_SIZE = 4
UNIXPATH = /tmp/gnunet-service-dht.sock
# This could be relaxed...
UNIX_MATCH_UID = YES
UNIX_MATCH_GID = YES
# DISABLE_SOCKET_FORWARDING = NO
# DEBUG = YES
# USERNAME = 
# MAXBUF =
# TIMEOUT =
# DISABLEV6 =
# BINDTO =
# REJECT_FROM =
# REJECT_FROM6 =
# PREFIX =


# Special option to disable DHT calling 'try_connect' (for testing)
DISABLE_TRY_CONNECT = NO


[dhtcache]
DATABASE = sqlite
QUOTA = 1 MB

