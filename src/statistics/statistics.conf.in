[statistics]
AUTOSTART = YES
@JAVAPORT@PORT = 2088
HOSTNAME = localhost
HOME = $SERVICEHOME
BINARY = gnunet-service-statistics
ACCEPT_FROM = 127.0.0.1;
ACCEPT_FROM6 = ::1;
UNIXPATH = $SERVICEHOME/gnunet-service-statistics.sock
UNIX_MATCH_UID = NO
UNIX_MATCH_GID = YES
