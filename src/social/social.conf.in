[social]
START_ON_DEMAND = @START_ON_DEMAND@
BINARY = gnunet-service-social
RUN_PER_USER = YES

UNIXPATH = $GNUNET_USER_RUNTIME_DIR/gnunet-service-social.sock
UNIX_MATCH_UID = YES
UNIX_MATCH_GID = YES

@UNIXONLY@PORT = 2116
HOSTNAME = localhost
ACCEPT_FROM = 127.0.0.1;
ACCEPT_FROM6 = ::1;

DATA_HOME = $GNUNET_DATA_HOME/social
