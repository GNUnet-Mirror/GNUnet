[secretsharing]
START_ON_DEMAND = NO
@JAVAPORT@PORT = 2114
HOSTNAME = localhost
BINARY = gnunet-service-secretsharing
ACCEPT_FROM = 127.0.0.1;
ACCEPT_FROM6 = ::1;
UNIXPATH = $GNUNET_RUNTIME_DIR/gnunet-service-secretsharing.sock
UNIX_MATCH_UID = YES
UNIX_MATCH_GID = YES
PREFIX = valgrind --leak-check=yes
