[chat]
AUTOSTART = YES
@UNIXONLY@ PORT = 2090
HOSTNAME = localhost
HOME = $SERVICEHOME
CONFIG = $DEFAULTCONFIG
BINARY = gnunet-service-chat
ACCEPT_FROM = 127.0.0.1;
ACCEPT_FROM6 = ::1;
UNIXPATH = /tmp/gnunet-service-chat.sock
UNIX_MATCH_UID = NO
UNIX_MATCH_GID = YES
