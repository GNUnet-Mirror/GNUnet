[gns]
AUTOSTART = YES
HOSTNAME = localhost
HOME = $SERVICEHOME
CONFIG = $DEFAULTCONFIG
BINARY = gnunet-service-gns
UNIXPATH = /tmp/gnunet-service-gns.sock
ZONEKEY_DIRECTORY = $SERVICEHOME/gns
ZONEKEY = $ZONEKEY_DIRECTORY/zonekey.zkey
HIJACK_DNS = NO

[fcfsd]
HTTPPORT = 18080
ZONEKEY = $SERVICEHOME/fcfsd/zonekey.zkey
