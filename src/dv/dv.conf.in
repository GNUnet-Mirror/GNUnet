BINARY = gnunet-service-dv
HOSTNAME = localhost
@UNIXONLY@ PORT = 2571
UNIXPATH = $GNUNET_RUNTIME_DIR/gnunet-service-dv.sock
UNIX_MATCH_UID = YES
UNIX_MATCH_GID = YES
